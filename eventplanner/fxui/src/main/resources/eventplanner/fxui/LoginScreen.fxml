            <Button fx:id="btnConfirm" layoutX="423.0" layoutY="187.0" mnemonicParsing="false" onAction="#handleConfirm" prefHeight="26.0" prefWidth="76.0" text="Confirm" />
            <Button fx:id="btnCancel" layoutX="516.0" layoutY="187.0" mnemonicParsing="false" onAction="#handleCancel" prefHeight="26.0" prefWidth="76.0" text="Cancel" />
